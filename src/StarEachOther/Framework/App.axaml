		<semi:SemiTheme Locale="zh-CN" />
	</Application.Styles>
	<Application.Resources>
		<sys:Double x:Key="ButtonDefaultFontSize">16</sys:Double>
		<FontWeight x:Key="ButtonDefaultFontWeight">Medium</FontWeight>
	</Application.Resources>
